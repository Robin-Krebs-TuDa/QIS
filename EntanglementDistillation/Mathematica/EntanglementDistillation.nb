Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Pauli", " ", "Matrices"}], "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"P0", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", "0"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"PX", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"1", ",", "0"}], "}"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"PY", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"-", "I"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"I", ",", "0"}], "}"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"PZ", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", "0"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"-", "1"}]}], "}"}]}], "}"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", 
    RowBox[{"Rotation", " ", "Matrices"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"RX", "[", "\[Theta]_", "]"}], ":=", 
    RowBox[{"MatrixExp", "[", 
     RowBox[{"I", " ", "\[Theta]", " ", 
      RowBox[{"PX", "/", "2"}]}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{"RY", "[", "\[Theta]_", "]"}], ":=", 
    RowBox[{"MatrixExp", "[", 
     RowBox[{"I", " ", "\[Theta]", " ", 
      RowBox[{"PY", "/", "2"}]}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{"RZ", "[", "\[Theta]_", "]"}], ":=", 
    RowBox[{"MatrixExp", "[", 
     RowBox[{"I", " ", "\[Theta]", " ", 
      RowBox[{"PZ", "/", "2"}]}], "]"}]}], "\n", "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Transforming", " ", "between", " ", "product", " ", "and", " ", "Bell", 
     " ", "basis"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"UBasisChange", "=", 
     RowBox[{
      RowBox[{"1", "/", 
       RowBox[{"\[Sqrt]", "2"}]}], 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"1", ",", "0", ",", "1", ",", "0"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "1", ",", "0", ",", "1"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "1", ",", "0", ",", 
          RowBox[{"-", "1"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", "0", ",", 
          RowBox[{"-", "1"}], ",", "0"}], "}"}]}], "}"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"ProdToBell", "[", "\[Rho]_", "]"}], ":=", 
    RowBox[{"Dot", "[", 
     RowBox[{
      RowBox[{"Transpose", "[", "UBasisChange", "]"}], ",", " ", "\[Rho]", 
      ",", " ", "UBasisChange"}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{"BellToProd", "[", "\[Rho]_", "]"}], ":=", 
    RowBox[{"Dot", "[", 
     RowBox[{"UBasisChange", ",", " ", "\[Rho]", ",", " ", 
      RowBox[{"Transpose", "[", "UBasisChange", "]"}]}], "]"}]}], "\n", "\n", 
   RowBox[{
    RowBox[{"Fidelity", "[", 
     RowBox[{"\[Rho]1_", ",", "\[Rho]2_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"Tr", "[", 
      RowBox[{"MatrixPower", "[", 
       RowBox[{
        RowBox[{"Dot", "[", 
         RowBox[{
          RowBox[{"MatrixPower", "[", 
           RowBox[{"\[Rho]1", ",", " ", 
            RowBox[{"1", "/", "2"}]}], "]"}], ",", " ", "\[Rho]2", ",", " ", 
          RowBox[{"MatrixPower", "[", 
           RowBox[{"\[Rho]1", ",", " ", 
            RowBox[{"1", "/", "2"}]}], "]"}]}], "]"}], ",", " ", 
        RowBox[{"1", "/", "2"}]}], "]"}], "]"}], "^", "2"}]}]}]}]], "Code",
