      RowBox[{
       SubscriptBox[
        TemplateBox[{"0"},
         "Bra"], "i"], " ", "\[TensorProduct]", 
       SubscriptBox["X", "j"]}]}], "+", 
     RowBox[{
      TemplateBox[{"1"},
       "Ket"], 
      RowBox[{
       SubscriptBox[
        TemplateBox[{"1"},
         "Bra"], "i"], " ", "\[TensorProduct]", 
       SubscriptBox["I", "j"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3862f8ca-73c2-49dd-b96b-7ce6c7fab202"],
 " , take the state ",
